  section#contact
    .contact-section
      .container
        h2.text-center CONTACT ME
        .row
          .col-md-6.col-md-offset-3
            h3.text-center.text-danger lisa.carlson85@gmail.com
        .row
          .col-md-12.icon-wrapper
            .icons
              a(href="https://gmail.com" target="_blank")
                i(class='glyphicon glyphicon-envelope')
